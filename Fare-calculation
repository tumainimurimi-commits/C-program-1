Description: Calculation of fare*/
#include <stdio.h>

int calculateFare(int distance) {
    int fare = distance * 50;
    return fare;
}

// Testing the function
int main() {
    int distance;
    
    printf("Enter distance traveled (km): ");
    scanf("%d", &distance);

    printf("Total Fare: KSh. %d\n", calculateFare(distance));

    return 0;
}

